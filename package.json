{
  "name": "tatiana-bot",
  "author": "ItsJhonAlex",
  "version": "0.0.1",
  "description": "A Discord bot built with Sapphire Framework and Drizzle ORM.",
  "repository": "https://github.com/ItsJhonAlex/tatiana-bot",
  "license": "MIT",
  "main": "index.ts",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts",
    "start": "bun index.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,yml,yaml,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,yml,yaml,md}\"",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:build": "docker compose build",
    "docker:logs": "docker compose logs -f bot"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.53.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@sapphire/framework": "^5.5.0",
    "@sapphire/plugin-hmr": "^3.0.2",
    "@sapphire/plugin-i18next": "^8.0.0",
    "@sapphire/plugin-logger": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.17.2",
    "postgres": "^3.4.8"
  }
}
